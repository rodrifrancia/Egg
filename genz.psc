SubProceso imprimirMatriz(matriz,i,j,dim)
	
	para i=0 Hasta dim-1 Hacer
		para j=0 hasta dim-1 Hacer
			escribir sin saltar matriz[i,j], " " 
		FinPara
		Escribir " "
	FinPara
	
FinSubProceso

SubProceso validarGenZ(matriz,dim)
	definir i,j,cont Como Entero
	definir aux,aux2 como caracter
	definir validacion1,validacion2 Como Logico
	validacion1=Verdadero
	validacion2=verdadero
	
	para i=1 Hasta dim-1 Hacer
		para j=1 Hasta dim-1 Hacer
			si i=j Entonces
				aux=matriz[i,j]
				si matriz[i-1,j-1]<>aux Entonces
					validacion1=falso
				FinSi
		    finsi
		FinPara
	FinPara

	j=dim-1
	
	para i=0 hasta dim-1 hacer
		si matriz[0,dim-1] <> matriz[i,j] Entonces
			validacion2 = Falso
		FinSi
			j=j-1
			
	finpara
	
	si validacion1=Verdadero y validacion2= Verdadero entonces
		Escribir "Contiene Gen Z"
	SiNo
		Escribir "No contiene Gen Z"
	FinSi
	
FinSubProceso
	